{
  "name": "ai-chat-monitoring-ui",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "build:check": "tsc && vite build",
    "preview": "vite preview --port 3000",
    "lint": "tsc --noEmit",
    "serve": "vite preview --port 3000",
    "validate:html": "echo \"HTML validation - build and check structure\"",
    "validate:a11y": "echo \"A11y validation - manual testing recommended\"",
    "validate:types": "tsc --noEmit",
    "validate:env": "node -e \"const vars = ['VITE_API_BASE_URL']; vars.forEach(v => { if (!process.env[v]) console.warn('Warning: ' + v + ' not set'); });\"",
    "validate": "npm run validate:env",
    "predev": "npm run validate:env"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@tensorflow/tfjs": "^4.22.0",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.454.0",
    "punycode": "^2.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^5.8.0",
    "recharts": "^3.3.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "msw": "^2.11.5",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^7.1.9"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
